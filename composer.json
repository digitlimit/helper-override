{
  "name": "digitlimit/helper-override",
  "description": "A Laravel package to override helper functions",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0",
    "composer-plugin-api": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Digitlimit\\HelperOverride\\": "src/"
    },
    "files": [
      "src/Helpers/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Digitlimit\\HelperOverride\\Providers\\HelperServiceProvider"
      ]
    },
    "class": "Digitlimit\\HelperOverride\\Plugin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
